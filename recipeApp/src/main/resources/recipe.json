  {
    "id": 3,
    "name": "Sweet and Sour Chicken Wrap",
    "category": "Lunch",
    "yield": "8 wraps",
    "ingredientsList": [
      {
        "ingredientAmt": "1/2 cup",
        "ingredientName": "mayonnaise"
      },
      {
        "ingredientAmt": "1/4 cup",
        "ingredientName": "finely chopped seedless cucumber"
      },
      {
        "ingredientAmt": "1 tbsp",
        "ingredientName": "honey (for sauce)"
      },
      {
        "ingredientAmt": "½ tsp",
        "ingredientName": "cayenne pepper (for sauce)"
      },
      {
        "ingredientAmt": "",
        "ingredientName": "ground pepper to taste"
      },
      {
        "ingredientAmt": "2 tbsp",
        "ingredientName": "olive oil"
      },
      {
        "ingredientAmt": "1 ½ pounds",
        "ingredientName": "skinless, boneless chicken breast halves - cut into thin strips"
      },
      {
        "ingredientAmt": "1 cup",
        "ingredientName": "thick and chunky salsa"
      },
      {
        "ingredientAmt": "1 tbsp",
        "ingredientName": "honey (for wrap)"
      },
      {
        "ingredientAmt": "½ tsp",
        "ingredientName": "cayenne pepper"
      },
      {
        "ingredientAmt": "8",
        "ingredientName": "(10 inch) flour tortillas"
      },
      {
        "ingredientAmt": "1 (10 ounce)",
        "ingredientName": "bad of baby spinach leaves"
      }
    ],
    "directionsList": [
      {
        "directionStep": 1,
        "directionInstruction": "Make the sauce: Mix mayonnaise, cucumber, honey, cayenne pepper, and black pepper together in a bowl until smooth. Cover and refrigerate until needed."
      },
      {
        "directionStep": 2,
        "directionInstruction": "Make the wraps: Heat olive oil in a skillet on medium-high heat. Add chicken strips; cook and stir until they start to turn golden and are no longer pink in the middle, about 8 minutes. Stir in salsa, honey, and cayenne pepper. Reduce the heat to medium-low; simmer, stirring occasionally, until flavors have blended, about 5 minutes."
      },
      {
        "directionStep": 3,
        "directionInstruction": "Stack tortillas, 4 at a time, in a microwave oven and heat until warm and pliable, 20 to 30 seconds per batch."
      },
      {
        "directionStep": 4,
        "directionInstruction": "Spread each tortilla with 1 tablespoon mayonnaise-cucumber sauce, top with a layer of spinach, and arrange about 1/2 cup of the chicken mixture on top."
      },
      {
        "directionStep": 5,
        "directionInstruction": "Fold the bottom of each tortilla up about 2 inches, and start rolling from the right side. When the wrap is halfway rolled, fold the top of the tortilla down, enclosing the filling, and continue rolling to make a tight, compact cylinder."
      }
    ]
  },
