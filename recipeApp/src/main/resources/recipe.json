    "id": 1,
    "name": "Pancake",
    "category": "Breakfast",
    "yield": "8",
    "ingredientsList": [
      {
        "ingredientAmt": "1.5 cups",
        "ingredientName": "all-purpose flour"
      },
      {
        "ingredientAmt": "3.5 tsp",
        "ingredientName": "baking powder"
      },
      {
        "ingredientAmt": "1 tbsp",
        "ingredientName": "white sugar"
      },
      {
        "ingredientAmt": "0.25 tsp",
        "ingredientName": "salt, or more to taste"
      },
      {
        "ingredientAmt": "1.25 cups",
        "ingredientName": "milk"
      },
      {
        "ingredientAmt": "3 tbsp",
        "ingredientName": "butter, melted"
      },
      {
        "ingredientAmt": "1",
        "ingredientName": "egg"
      }
    ],
    "directionsList": [
      {
        "directionStep": 1,
        "directionInstruction": "Sift flour, baking powder, sugar, and salt together in a large bowl. Make a well in the center and add milk, melted butter, and egg; mix until smooth."
      },
      {
        "directionStep": 2,
        "directionInstruction": "Heat a lightly oiled griddle or pan over medium-high heat. Pour or scoop the batter onto the griddle, using approximately 1/4 cup for each pancake; cook until bubbles form and the edges are dry, about 2 to 3 minutes. Flip and cook until browned on the other side. Repeat with remaining batter."
      }
    ]
  },

